<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tabsandtrack"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.tnt.activity.HomeActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.tnt.activity.LoginActivity"
            android:label="@string/title_activity_login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tnt.activity.SignUpActivity"
            android:label="@string/title_activity_sign_up" >
        </activity>
        <activity
            android:name="com.tnt.activity.AddAccountActivity"
            android:label="@string/title_activity_add_account" >
        </activity>
        <activity
            android:name="com.tnt.dboperation.UserDetailsDbOperations"
            android:label="@string/title_activity_user_details_db_operations" >
        </activity>
    </application>

</manifest>
